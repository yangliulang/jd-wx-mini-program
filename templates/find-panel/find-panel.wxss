.find-panel-one{
  background-color: #fff;
  margin-bottom: 28rpx;
}
.find-panel-one .name{
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 0 30rpx;
}
.find-panel-one .img{
  width: 100%;
  height: 370rpx;
}
.find-panel-one .detail{
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:0 30rpx;
  color: #999;
}
.find-panel-one .detail view{
  display: flex;
  align-items: center;
}
.find-panel-one .detail .icon{
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  border-radius: 50%;
  margin-right: 10rpx;
}

.find-page-tow{
  background-color: #fff;
  margin-bottom: 28rpx;
  display: flex;
}
.find-page-tow .left,.find-page-tow .right{
  width: 50%;
  height: 240rpx;
}
.find-page-tow .left{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding:30rpx;
  box-sizing: border-box;
}

.find-page-tow .left .detail{
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #999;
}
.find-page-tow .left .detail view{
  display: flex;
  align-items: center;
  font-size: 20rpx;
}
.find-page-tow .left .detail .icon{
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  border-radius: 50%;
  margin-right: 10rpx;
}

.find-page-tow .left .name{
  overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.find-page-tow .right image{
  width: 340rpx;
  height: 240rpx;
}